{
   "action": {
      "default_popup": "legacy/popup/index.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "contexts/all/content-script/import-index.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "contexts/privileged/content-script/import-index.js" ],
      "matches": [ "https://*.truffle.vip/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "legacy/content/embed/import-index.js" ],
      "matches": [ "*://*.twitch.tv/*", "*://*.youtube.com/*", "*://*.patreon.com/*", "*://*.ludwig.gg/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "legacy/content/twitch/import-index.js" ],
      "matches": [ "*://*.twitch.tv/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "legacy/content/youtube/import-index.js" ],
      "matches": [ "*://*.youtube.com/*" ],
      "run_at": "document_idle"
   }, {
      "js": [ "legacy/content/mogultv/import-index.js" ],
      "matches": [ "*://v2.truffle.vip/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Enhance your livestream viewing experience",
   "devtools_page": "devtools/entry.html",
   "externally_connectable": {
      "ids": [ "pmnmpgjfacmjcnfigcmgfipemjpggmeg" ],
      "matches": [ "*://*.truffle.vip/*" ]
   },
   "host_permissions": [ "http://*/*", "https://*/*" ],
   "icons": {
      "128": "assets/icon128.png",
      "16": "assets/icon16.png",
      "48": "assets/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Truffle",
   "permissions": [ "cookies", "storage", "unlimitedStorage", "gcm", "notifications", "declarativeNetRequestWithHostAccess" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.3.6",
   "web_accessible_resources": [ {
      "matches": [ "http://*/*", "https://*/*" ],
      "resources": [ "contexts/all/injected-script/index.js", "devtools/index.html", "legacy/injected/all/index.js" ]
   }, {
      "matches": [ "*://*.youtube.com/*" ],
      "resources": [ "legacy/injected/youtube/index.js", "legacy/injected/youtube/quick_actions.js", "legacy/popup/index.html" ]
   }, {
      "matches": [ "*://*.twitch.tv/*" ],
      "resources": [ "legacy/injected/twitch/index.js", "legacy/injected/twitch/quick_actions.js" ]
   }, {
      "matches": [ "http://*/*", "https://*/*", "https://*.truffle.vip/*", "*://*.twitch.tv/*", "*://*.youtube.com/*", "*://*.patreon.com/*", "*://*.ludwig.gg/*", "*://v2.truffle.vip/*" ],
      "resources": [ "chunks/*-*.js", "contexts/all/content-script/index.js", "contexts/privileged/content-script/index.js", "legacy/content/embed/index.js", "legacy/content/twitch/index.js", "legacy/content/youtube/index.js", "legacy/content/mogultv/index.js" ]
   } ]
}
